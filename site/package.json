{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "notion:pull": "export $(cat ../.env | xargs) && node ./bin/notionPullPages.js",
    "lint": "eslint .",
    "stylelint": "stylelint .",
    "style-lint": "stylelint components/**/*.scss pages/**/*.scss **/*.scss",
    "next-lint": "next lint",
    "prettier": "prettier -w .",
    "pre-commit": "lint-staged",
    "install-husky": "cd .. && husky install site/.husky",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "resolutions": {
    "webpack": "^5"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "12.2.2",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-gist": "^1.2.4",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@notionhq/client": "^2.0.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "eslint": "8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "storybook-tailwind-dark-mode": "^1.0.12",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^5.0.0",
    "tailwindcss": "^3.1.6",
    "uuid": "^8.3.2",
    "webpack": "^5.73.0"
  }
}
