{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "style-lint": "stylelint components/**/*.scss pages/**/*.scss **/*.scss",
    "next-lint": "next lint",
    "prettier": "prettier -w .",
    "pre-commit": "lint-staged",
    "install-husky": "cd .. && husky install site-new/.husky"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "stylelint"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "eslint": "8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6"
  }
}
